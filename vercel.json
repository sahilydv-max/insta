{
    "version": 2,
    "builds": [
      {
        "src": "bin/www",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "bin/www"
      }
    ],
     "env": {
    "NODE_ENV": "production",
    "MY_ENV_VAR": "value" 
  }
  }
  
